{
  "expo": {
    "name": "QR Scanner",
    "slug": "qr-scanner",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "scheme": "qrscanner",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.cubion.qrscanner",
      "usesIcloudStorage": true,
      "infoPlist": {
        "NSCameraUsageDescription": "QR 코드 스캔을 위해 카메라 접근이 필요합니다.",
        "NSPhotoLibraryUsageDescription": "QR 코드에 로고를 추가하기 위해 사진 접근이 필요합니다.",
        "NSLocationWhenInUseUsageDescription": "위치 QR 코드 생성을 위해 현재 위치 접근이 필요합니다.",
        "LSApplicationQueriesSchemes": ["googlechrome", "googlechromes", "kakaokompassauth", "kakaolink", "naversearchapp", "naversearchthirdlogin"],
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        },
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": ["com.googleusercontent.apps.585698187056-rfr4k7k4vkb9rjhngb0tdnh5afqgogot"]
          }
        ]
      },
      "entitlements": {
        "com.apple.developer.icloud-container-identifiers": ["iCloud.com.cubion.qrscanner"],
        "com.apple.developer.icloud-services": ["CloudDocuments", "CloudKit"],
        "com.apple.developer.ubiquity-container-identifiers": ["iCloud.com.cubion.qrscanner"],
        "com.apple.developer.ubiquity-kvstore-identifier": "$(TeamIdentifierPrefix)com.cubion.qrscanner"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.cubion.qrscanner",
      "usesCleartextTraffic": true,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBbUKvaAOEmB3EReIKjqchon5pGSKIW4mQ"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "react-native-cloud-store",
        {
          "iCloudContainerEnvironment": "Production"
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "QR 코드 스캔을 위해 카메라 접근이 필요합니다."
        }
      ],
      [
        "expo-location",
        {
          "locationWhenInUsePermission": "위치 QR 코드 생성을 위해 현재 위치 접근이 필요합니다."
        }
      ],
      [
        "react-native-vision-camera",
        {
          "cameraPermissionText": "QR 코드 스캔을 위해 카메라 접근이 필요합니다.",
          "enableMicrophonePermission": false,
          "enableCodeScanner": true
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "QR 코드에 로고를 추가하기 위해 사진 접근이 필요합니다."
        }
      ],
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-3940256099942544~3347511713",
          "iosAppId": "ca-app-pub-3940256099942544~1458002511",
          "delayAppMeasurementInit": true,
          "userTrackingUsageDescription": "맞춤형 광고를 제공하기 위해 사용됩니다."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "serverUrl": "https://scanview.app",
      "kakao": {
        "nativeAppKey": "YOUR_KAKAO_NATIVE_APP_KEY"
      },
      "naver": {
        "clientId": "YOUR_NAVER_CLIENT_ID",
        "serviceUrlScheme": "qrscanner"
      },
      "google": {
        "webClientId": "585698187056-3tqjnjbcdidddn9ddvp2opp0mgj7tgd4.apps.googleusercontent.com",
        "iosClientId": "585698187056-rfr4k7k4vkb9rjhngb0tdnh5afqgogot.apps.googleusercontent.com",
        "androidClientId": "585698187056-c245sfancbg2e2djpjn6q0j945f0muff.apps.googleusercontent.com"
      }
    }
  }
}
